# JSONPlaceholder API base URL
API_URL=https://jsonplaceholder.typicode.com

# Output CSV file path (relative or absolute)
CSV_OUTPUT_PATH=output/users.csv

# Path to the user data CSV file (relative or absolute)
USER_CSV_PATH=./data/users.csv

# Backend server port (optional, default: 3001)
PORT=3001

# Logging level (info, warn, error)
LOG_LEVEL=info

# --- Frontend (client) ---
# Place these in client/.env and client/.env.example
REACT_APP_API_URL=http://localhost:3001
REACT_APP_WS_URL=http://localhost:3001